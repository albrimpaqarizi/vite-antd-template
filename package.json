{
  "name": "vite-react-ts-antd-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "dev": "vite --mode dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc",
    "lint:check": "next lint",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --cache --fix",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --write",
    "prettier:check": "prettier --config .prettierrc \"src/**/*.+(ts|js|jsx|tsx)\" --check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-react": "^1.0.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "lass": "^3.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "less-vars-to-js": "^1.3.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-plugin-imp": "^2.1.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run prettier"
    ],
    "*.{json,md}": [
      "npm run prettier"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
