{
  "name": "vite-react-ts-antd-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "serve": "serve -s build",
    "dev": "vite --mode dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc",
    "lint:check": "next lint",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --cache --fix",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --write",
    "prettier:check": "prettier --config .prettierrc \"src/**/*.+(ts|js|jsx|tsx)\" --check",
    "prepare": "husky install && chmod ug+x .husky/*"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@loadable/component": "^5.15.2",
    "antd": "^4.17.1",
    "axios": "^0.26.1",
    "less": "^4.1.2",
    "path": "^0.12.7",
    "promise-timeout": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "sass": "^1.50.0",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.4"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/loadable__component": "^5.13.4",
    "@types/promise-timeout": "^1.3.0",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.10",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite-plugin-environment": "^1.1.1",
    "vite-plugin-imp": "^2.1.7",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run prettier"
    ],
    "*.{json,md}": [
      "npm run prettier"
    ]
  }
}
